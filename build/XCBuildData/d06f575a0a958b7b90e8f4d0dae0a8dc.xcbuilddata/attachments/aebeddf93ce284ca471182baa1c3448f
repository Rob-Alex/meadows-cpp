#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/robbiealexander/Projects/Cpp/meadows/shaders
  xcrun -sdk macosx metallib /Users/robbiealexander/Projects/Cpp/meadows/shaders/compute.air -o /Users/robbiealexander/Projects/Cpp/meadows/shaders/compute.metallib
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/robbiealexander/Projects/Cpp/meadows/shaders
  xcrun -sdk macosx metallib /Users/robbiealexander/Projects/Cpp/meadows/shaders/compute.air -o /Users/robbiealexander/Projects/Cpp/meadows/shaders/compute.metallib
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/robbiealexander/Projects/Cpp/meadows/shaders
  xcrun -sdk macosx metallib /Users/robbiealexander/Projects/Cpp/meadows/shaders/compute.air -o /Users/robbiealexander/Projects/Cpp/meadows/shaders/compute.metallib
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/robbiealexander/Projects/Cpp/meadows/shaders
  xcrun -sdk macosx metallib /Users/robbiealexander/Projects/Cpp/meadows/shaders/compute.air -o /Users/robbiealexander/Projects/Cpp/meadows/shaders/compute.metallib
fi

